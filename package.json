{
  "name": "react-render-destination",
  "version": "1.0.1",
  "description": "A React/ReactNative library based on useSyncExternalStore to render things from some places into others",
  "author": "Naoufal ELYOUSSOUFI <n.elyousfi@gmail.com> (https://github.com/nelyousfi)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "keywords": [
    "react",
    "react-native",
    "render",
    "deatination",
    "portal",
    "gateway"
  ],
  "homepage": "https://github.com/nelyousfi/react-render-destination",
  "repository": {
    "type": "git",
    "url": "https://github.com/nelyousfi/react-render-destination"
  },
  "scripts": {
    "lint": "eslint \"src/index.{ts,tsx}\"",
    "build": "tsc && mkdir -p example/ReactNativeExample/node_modules/react-render-destination && cp -r package.json dist example/ReactNativeExample/node_modules/react-render-destination"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.1.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.0.3",
    "eslint": "^8.23.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "4",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "react": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && rm -rf dist && mkdir dist/ && yarn run build"
    }
  },
  "dependencies": {
    "use-sync-external-store": "^1.2.0"
  }
}
